{
  "info": {
    "name": "Woloo API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "GET /lessons",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/lessons",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "lessons"
          ]
        }
      }
    },
    {
      "name": "GET /search",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/search?q={{query}}",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "search"
          ],
          "query": [
            {
              "key": "q",
              "value": "{{query}}"
            }
          ]
        }
      }
    },
    {
      "name": "POST /orders",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Test User\",\n  \"phone\": \"1234567890\",\n  \"items\": [\n    { \"lessonId\": \"<replace>\", \"quantity\": 1 }\n  ]\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/orders",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "orders"
          ]
        }
      }
    },
    {
      "name": "PUT /lessons/:id",
      "request": {
        "method": "PUT",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"spaces\": 4\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/lessons/:id",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "lessons",
            ":id"
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000"
    },
    {
      "key": "query",
      "value": "a"
    }
  ]
}



